{
  "name": "@rlanz/bull-queue",
  "version": "2.0.0-0",
  "description": "Queue system based on BullMQ for AdonisJS",
  "homepage": "https://github.com/romainlanz/adonis-bull-queue#readme",
  "engines": {
    "node": ">=18.16.0"
  },
  "license": "MIT",
  "type": "module",
  "keywords": [
    "adonisjs",
    "bullmq",
    "queue"
  ],
  "author": "Romain Lanz <romain.lanz@pm.me>",
  "main": "build/main.js",
  "files": [
    "build/commands",
    "build/providers",
    "build/services",
    "build/src",
    "build/stubs",
    "build/index.js",
    "build/index.d.ts",
    "build/configure.js",
    "build/configure.d.ts"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands/*": "./build/commands/*.js",
    "./services/*": "./build/services/*.js"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "npm run lint && npm run clean && npm run build-only && npm run copyfiles",
    "build-only": "tsc",
    "clean": "del-cli build",
    "copyfiles": "copyfiles \"stubs/**/**/*.stub\" build",
    "prepublishOnly": "npm run build",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@poppinss/utils": "^6.5.0-3",
    "@sindresorhus/is": "^5.5.2",
    "bullmq": "^4.5.0"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.1.5-8",
    "@adonisjs/eslint-config": "^1.1.8",
    "@adonisjs/prettier-config": "^1.1.8",
    "@adonisjs/tsconfig": "^1.1.8",
    "@swc/core": "^1.3.70",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.45.0",
    "np": "^8.0.4",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.1.5-8"
  },
  "publishConfig": {
    "tag": "next",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romainlanz/adonis-bull-queue.git"
  },
  "bugs": {
    "url": "https://github.com/romainlanz/adonis-bull-queue/issues"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config"
}